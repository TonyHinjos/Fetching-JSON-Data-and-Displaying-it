<div class="wrapper">
<div class="profile">
<table width = "60%"id= "entrydata" border="1">
<thead>
        <th>ID</th>
        <th>Name</th>
        
    </thead>
  <tbody>

   </tbody>
   <tfoot>
            <tr>
               <th>ID</th>
                <th>Name</th>              
            </tr>
        </tfoot>
</table>

